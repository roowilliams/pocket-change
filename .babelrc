{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source",
        [
          "module-resolver",
          {
            "root": ["./"],
            "alias": {
              "components": "./components",
              "utils": "./utils",
              "constants": "./constants",
              "navigation": "./navigation",
              "screens": "./screens",
              "assets": "./assets"
            }
          }
        ]
      ]
    }
  }
}
